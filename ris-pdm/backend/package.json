{
  "name": "ris-performance-dashboard-backend",
  "version": "1.0.0",
  "description": "Backend API for RIS Performance Dashboard with Azure DevOps integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "example:azure": "node src/examples/azureDevOpsUsage.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "build": "echo 'No build step required for Node.js'",
    "docker:build": "docker build -t ris-dashboard-backend .",
    "docker:run": "docker run -p 3001:3001 ris-dashboard-backend"
  },
  "keywords": [
    "azure-devops",
    "performance-dashboard",
    "api",
    "node.js",
    "express",
    "metrics"
  ],
  "author": "RIS Product Team",
  "license": "MIT",
  "dependencies": {
    "@azure/msal-node": "^3.6.3",
    "bcryptjs": "^2.4.3",
    "bull": "^4.10.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.1",
    "jspdf": "^3.0.1",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.1",
    "puppeteer": "^24.14.0",
    "redis": "^4.6.7",
    "socket.io": "^4.8.1",
    "winston": "^3.9.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/supertest": "^6.0.3",
    "c8": "^10.1.3",
    "eslint": "^8.44.0",
    "eslint-config-node": "^4.1.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.6.1",
    "jest-extended": "^6.0.0",
    "nock": "^14.0.6",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3",
    "supertest-session": "^5.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2021": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "error",
      "no-undef": "error"
    }
  }
}
