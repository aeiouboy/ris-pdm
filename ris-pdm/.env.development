# Development Environment Configuration
COMPOSE_PROJECT_NAME=ris-dashboard-dev
COMPOSE_FILE=docker-compose.dev.yml

# Backend Configuration
NODE_ENV=development
PORT=3003
CORS_ORIGIN=http://localhost:5173
DISABLE_AZURE_DEVOPS=false
SKIP_AUTH=true
DISABLE_REDIS=true
DISABLE_PERFORMANCE_MONITORING=true 

# Azure DevOps Configuration - Working credentials
AZURE_DEVOPS_ORG=centralgroup
AZURE_DEVOPS_PROJECT=Product - Partner Management Platform
AZURE_DEVOPS_TEAM=PMP Developer Team
AZURE_DEVOPS_PAT=your_azure_devops_personal_access_token_here

# Multi-project support
AZURE_DEVOPS_FETCH_ALL_PROJECTS=false

# Rate Limiting (relaxed for development)
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=1000

# Real-time Service Configuration  
REALTIME_POLLING_INTERVAL=10000
ENABLE_INCREMENTAL_UPDATES=true
MAX_WEBSOCKET_CLIENTS=100
ENABLE_CHANGE_DETECTION=true
DISABLE_CACHE_WARMUP=true

# Database Configuration
DATABASE_URL=postgres://user:password@localhost:5432/ris_dashboard_dev

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=your_redis_password_here

# Frontend Configuration
VITE_API_BASE_URL=http://localhost:3003
VITE_WS_URL=ws://localhost:3003
VITE_ENV=development

# Monitoring Configuration
GRAFANA_USER=admin
GRAFANA_PASSWORD=your_grafana_password_here